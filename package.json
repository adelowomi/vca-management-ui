{
  "name": "management-ui",
  "author": "@qaltrakadmin",
  "license": "MIT",
  "version": "1.0.0",
  "scripts": {
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "dev": "next dev",
    "build": "next build",
    "format": "prettier --write .",
    "generate": "plop",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "lint-fix": "eslint . --ext ts --ext tsx --ext js --fix",
    "start": "next start",
    "analyze": "ANALYZE=true next build",
    "storybook": "start-storybook -p 6006",
    "type-check": "tsc --pretty --noEmit",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test": "jest",
    "test-all": "npm run lint && npm run type-check && npm test",
    "test:e2e": "start-server-and-test dev http://localhost:3000 cy:open",
    "test:e2e:ci": "start-server-and-test dev http://localhost:3000 cy:run"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run type-check && npm test && npm run test:e2e:ci"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx|js|json)": [
      "npm run lint",
      "npm run format"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.1.3",
    "@auth0/nextjs-auth0": "^1.0.0",
    "@headlessui/react": "^1.0.0",
    "@heroicons/react": "^1.0.1",
    "@hookform/resolvers": "^2.0.0",
    "@svgr/webpack": "^5.5.0",
    "@tailwindcss/postcss7-compat": "^2.0.2",
    "@types/styletron-engine-atomic": "^1.1.0",
    "@types/styletron-react": "^5.0.2",
    "@types/styletron-standard": "^2.0.1",
    "autoprefixer": "^9.8.6",
    "baseui": "^9.106.3",
    "bulma": "^0.9.2",
    "graphql": "^15.4.0",
    "moment": "^2.29.1",
    "next": "latest",
    "postcss": "^7.0.35",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.0.0",
    "react-intl": "^5.10.9",
    "react-select": "^3.1.1",
    "react-use": "^17.2.3",
    "styled-components": "^5.0.0",
    "styletron-engine-atomic": "^1.4.6",
    "styletron-react": "^5.2.7",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.0.2",
    "use-media": "^1.4.0",
    "zod": "^1.11.13"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@cypress/code-coverage": "^3.8.8",
    "@netlify/plugin-nextjs": "^1.0.3",
    "@storybook/addon-actions": "^6.2.8",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-knobs": "^6.2.8",
    "@storybook/addon-links": "^6.2.8",
    "@storybook/addon-viewport": "^6.2.8",
    "@storybook/react": "^6.2.8",
    "@testing-library/cypress": "^7.0.3",
    "@testing-library/jest-dom": "^5.11.8",
    "@testing-library/react": "^10.0.1",
    "@testing-library/user-event": "^12.6.0",
    "@types/jest": "^26.0.19",
    "@types/mocha": "^8.2.0",
    "@types/node": "^13.9.5",
    "@types/react": "^16.9.27",
    "@types/styled-components": "^5.1.7",
    "@types/testing-library__cypress": "^5.0.8",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "babel-jest": "^25.2.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.12.0",
    "babel-preset-react-app": "^10.0.0",
    "core-js": "^3.8.1",
    "critters": "0.0.6",
    "cypress": "^6.2.0",
    "encoding": "^0.1.13",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-baseui": "^9.106.3",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-testing-library": "^3.10.1",
    "husky": "^4.3.8",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.2.3",
    "jest-watch-typeahead": "^0.5.0",
    "lint-staged": "^10.0.10",
    "plop": "^2.7.4",
    "prettier": "^2.0.2",
    "react-query": "^3.5.5",
    "sb": "^6.1.21",
    "start-server-and-test": "^1.11.7",
    "typescript": "^4.1.0"
  }
}

{
  "name": "management-ui",
  "author": "@qaltrakadmin",
  "license": "MIT",
  "version": "1.0.0",
  "scripts": {
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "dev": "next dev",
    "build": "next build",
    "format": "prettier --write .",
    "generate": "plop",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "lint-fix": "eslint . --ext ts --ext tsx --ext js --fix",
    "start": "next start",
    "analyze": "ANALYZE=true next build",
    "type-check": "tsc --pretty --noEmit",
    "test": "jest",
    "test-all": "npm run lint && npm run type-check && npm test",
    "test:e2e": "start-server-and-test dev http://localhost:3000 cy:open",
    "test:e2e:ci": "start-server-and-test dev http://localhost:3000 cy:run",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run type-check && npm test"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx|js|json)": [
      "npm run lint",
      "npm run format"
    ]
  },
